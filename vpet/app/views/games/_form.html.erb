<%= form_for([@pet, @game]) do |f| %>

<div class="field">
    <%= f.label :g_name %><br />
    <%= f.text_field :g_name %>
</div>

<div class="field">
    <%= f.label :g_des %><br />
    <%= f.text_field :g_des %>
</div>

<div class="field">
    <%= f.label :money %><br />
    <%= f.text_field :money %>
</div>

<div class="field">
    <%= f.label :cost %><br />
    <%= f.text_field :cost %>
</div>

<div class="actions">
    <%= f.submit %>
</div>

<% end %>
